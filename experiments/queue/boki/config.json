{
    "machines": {
        "bokiqueue-gateway": { "type": "c5d.2xlarge", "role": "manager" },
        "bokiqueue-sequencer-1": { "type": "c5d.xlarge", "role": "worker", "labels": ["sequencer_node=true"] },
        "bokiqueue-sequencer-2": { "type": "c5d.xlarge", "role": "worker", "labels": ["sequencer_node=true"] },
        "bokiqueue-sequencer-3": { "type": "c5d.xlarge", "role": "worker", "labels": ["sequencer_node=true"] },
        "bokiqueue-engine-1": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "bokiqueue-engine-2": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "bokiqueue-engine-3": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "bokiqueue-engine-4": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "bokiqueue-engine-5": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "bokiqueue-engine-6": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "bokiqueue-engine-7": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "bokiqueue-engine-8": { "type": "c5d.2xlarge", "role": "worker", "labels": [ "engine_node=true" ] },
        "bokiqueue-storage-1": { "type": "c5d.2xlarge", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true"] },
        "bokiqueue-storage-2": { "type": "c5d.2xlarge", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true"] },
        "bokiqueue-storage-3": { "type": "c5d.2xlarge", "role": "worker", "mount_instance_storage": "nvme1n1", "labels": ["storage_node=true"] },
        "bokiqueue-client": { "type": "c5d.xlarge", "role": "client" }
    },
    "services": {
        "zookeeper": { "placement": "bokiqueue-gateway" },
        "zookeeper-setup": { "placement": "bokiqueue-gateway" },
        "boki-controller": { "placement": "bokiqueue-gateway" },
        "boki-gateway": { "placement": "bokiqueue-gateway" },
        "boki-storage": { "placement_label": "storage_node", "replicas": 3 },
        "boki-sequencer": { "placement_label": "sequencer_node", "replicas": 3 },
        "boki-engine": { "placement_label": "engine_node", "replicas": 8 },
        "consumer-fn": { "placement_label": "engine_node", "replicas": 8 },
        "producer-fn": { "placement_label": "engine_node", "replicas": 8 }
    }
}
